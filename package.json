{
  "name": "cold-outreach-crm",
  "version": "1.0.0",
  "description": "Cold Outreach Management Webapp",
  "main": "server/server.js",
  "scripts": {
    "postinstall": "npx prisma generate --schema=server/prisma/schema.prisma",
    "start": "node server/server.js",
    "dev": "node --watch server/server.js",
    "seed": "node server/prisma/seed.js",
    "db:migrate": "npx prisma migrate dev --schema=server/prisma/schema.prisma",
    "db:push": "npx prisma db push --schema=server/prisma/schema.prisma",
    "db:generate": "npx prisma generate --schema=server/prisma/schema.prisma"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeeLars/cold-outreach-CRM.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "dotenv": "^17.2.4",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "prisma": "^6.4.1"
  }
}
